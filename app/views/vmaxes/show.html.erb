<%- model_class = Vmax -%>
<div class="page-header">
  <h1><%=t '.title', :default => Vmax %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:serial_number) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.serial_number %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:system_name) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.system_name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:enginuity_code) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.enginuity_code %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:target_code) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.target_code %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ship_date) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.ship_date %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:install_date) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.install_date %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:esrs_enabled) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.esrs_enabled %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:contract_number) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.contract_number %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:contract_expiration) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.contract_expiration %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:raw_tb) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.raw_tb %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:engine_count) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.engine_count %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:openscale) %></dt>
      <dd><pre class="prettyprint"><%= @vmax.openscale %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              vmaxes_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_vmax_path(@vmax), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              vmax_path(@vmax),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

